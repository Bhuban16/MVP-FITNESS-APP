<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2A2828"
    tools:context=".ProfileActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="151dp"
        android:layout_height="136dp"
        android:layout_marginTop="652dp"
        android:layout_marginEnd="308dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ellipse1" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="157dp"
        android:layout_height="160dp"
        android:layout_marginStart="308dp"
        android:layout_marginTop="652dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ellipse1" />

    <Button
        android:id="@+id/btnChangePassword"
        android:layout_width="125dp"
        android:layout_height="44dp"
        android:background="@drawable/rectangle_2"
        android:backgroundTint="#FFFFFF"
        android:text="change password"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnProfileUpdate"
        app:layout_constraintVertical_bias="0.428" />

    <ImageView
        android:id="@+id/ivProfilePic"
        android:layout_width="158dp"
        android:layout_height="140dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.055"
        app:srcCompat="@drawable/group_2" />

    <TextView
        android:id="@+id/tvProfileName"
        android:layout_width="370dp"
        android:layout_height="47dp"
        android:background="@drawable/rectangle_2"
        android:drawableStart="@drawable/ic_baseline_account_circle_24"
        android:drawablePadding="10dp"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:text="Name:"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ivProfilePic"
        app:layout_constraintVertical_bias="0.077" />

    <TextView
        android:id="@+id/tvProfileEmail"
        android:layout_width="370dp"
        android:layout_height="47dp"
        android:background="@drawable/rectangle_2"
        android:drawableStart="@drawable/ic_baseline_email_24"
        android:drawablePadding="10dp"
        android:ems="10"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:text="Email:"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/tvProfileName"
        app:layout_constraintVertical_bias="0.221" />

    <Button
        android:id="@+id/btnProfileUpdate"
        android:layout_width="127dp"
        android:layout_height="39dp"
        android:layout_marginBottom="88dp"
        android:background="@drawable/rectangle_2"
        android:backgroundTint="#FFFFFF"
        android:text="Edit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tvProfilePhoneNumber"
        android:layout_width="370dp"
        android:layout_height="47dp"
        android:background="@drawable/rectangle_2"
        android:drawableStart="@drawable/ic_baseline_local_phone_24"
        android:drawablePadding="10dp"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:text="Phone Number:"
        android:textColor="#020202"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/tvProfileEmail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvProfileName"
        app:layout_constraintVertical_bias="0.457" />

    <TextView
        android:id="@+id/tvProfileAge"
        android:layout_width="173dp"
        android:layout_height="43dp"
        android:background="@drawable/rectangle_2"
        android:drawableStart="@drawable/ic_baseline_cake_24"
        android:drawablePadding="10dp"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:text="Age:"
        android:textColor="#020202"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btnProfileUpdate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.088"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvProfileEmail"
        app:layout_constraintVertical_bias="0.106">

    </TextView>

    <TextView
        android:id="@+id/tvProfileWeight"
        android:layout_width="173dp"
        android:layout_height="43dp"
        android:layout_marginTop="63dp"
        android:background="@drawable/rectangle_2"
        android:drawableStart="@drawable/img_ideal_body_weight"
        android:drawablePadding="10dp"
        android:drawableTint="@color/dark"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:text="Weight:"
        android:textColor="#020202"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btnProfileUpdate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toEndOf="@+id/tvProfileAge"
        app:layout_constraintTop_toTopOf="@+id/tvProfileEmail"
        app:layout_constraintVertical_bias="0.0">

    </TextView>

    <TextView
        android:id="@+id/tvProfileHeight"
        android:layout_width="269dp"
        android:layout_height="45dp"
        android:background="@drawable/rectangle_2"
        android:drawableStart="@drawable/ic_baseline_height_24"
        android:drawablePadding="10dp"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:text="Height:"
        android:textColor="#020202"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btnProfileUpdate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvProfileAge"
        app:layout_constraintVertical_bias="0.183">

    </TextView>

    <TextView
        android:id="@+id/tvProfileGender"
        android:layout_width="268dp"
        android:layout_height="44dp"
        android:background="@drawable/rectangle_2"
        android:drawableStart="@drawable/ic_baseline_height_24"
        android:drawablePadding="10dp"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:text="Gender:"
        android:textColor="#020202"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btnProfileUpdate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvProfileWeight"
        app:layout_constraintVertical_bias="0.787">

    </TextView>


</androidx.constraintlayout.widget.ConstraintLayout>